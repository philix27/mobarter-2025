{
  "name": "@repo/api",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
